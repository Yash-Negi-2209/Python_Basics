   "source": [
    "n = int(input(\"Enter a number:\"))\n",
    "temp = n\n",
    "rev = 0\n",
    "while( n > 0 ):\n",
    "    dig = n % 10\n",
    "    rev = rev*10 + dig\n",
    "    n = n // 10\n",
    "if( temp == rev ):\n",
    "    print(\"The number is palindrome!\")\n",
    "else:\n",
    "    print(\"Not a palindrome!\")"
   ]
  }
