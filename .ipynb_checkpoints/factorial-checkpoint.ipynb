   "source": [
    "n = int(input(\"ENTER A NUMBER WHOSE FACTORIAL IS TO BE PRINTED : \"))\n",
    "if n > 0:\n",
    "    fact = 1\n",
    "    while n >= 1:\n",
    "        fact = fact*n\n",
    "        n = n - 1 \n",
    "    print(\"THE FACTORIAL IS : \" + str(fact))\n",
    "else:\n",
    "    print(\"INVALID INPUT\")"
   ]
  }
